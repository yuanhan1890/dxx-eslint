{
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "airbnb", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "eqeqeq": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "prefer-template": 0,
    "react/sort-comp": 0,
    "no-plusplus": 0,
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}
